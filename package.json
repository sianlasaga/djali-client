{
  "name": "djali-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "@types/qrcode.react": "^0.8.2",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/react-tagsinput": "^3.19.3",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "qrcode.react": "^0.9.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "react-select": "^3.0.4",
    "react-select-search": "^0.9.6",
    "react-tagsinput": "^3.19.0",
    "search-index": "^1.0.6",
    "slugify": "^1.3.4",
    "typescript": "^3.4.5",
    "uikit": "^3.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:win:develop": "set \"REACT_APP_ENVIRONMENT=development\" && npm start",
    "start:unix:develop": "REACT_APP_ENVIRONMENT=development npm start",
    "start:unix:stage": "REACT_APP_ENVIRONMENT=staging npm start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint": "tslint -c tslint.json src/**/*.{ts,tsx} --fix --format verbose"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint -c tslint.json"
    ]
  }
}
